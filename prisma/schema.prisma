// schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id      Int         @id @default(autoincrement())
  nome    String      @unique
  preco   Int
  unidade String
  role    ProductRule @default(product)
}

enum ProductRule {
  product
}
